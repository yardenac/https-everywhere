<ruleset name="OWASP" platform="mixedcontent">

	<target host="owasp.org" />
	<target host="*.owasp.org" />
		<exclusion pattern="^https?://(?:owasp4|sl)\.owasp\.org/" />

	<securecookie host="^(?:.*\.)?owasp\.org$" name=".*" />

	<rule from="^http://([^/:@\.]+\.)?owasp\.org/"
		to="https://$1owasp.org/" />

</ruleset>
